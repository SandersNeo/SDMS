	<calculatedField>
		<dataPath>ВремяПоТабелю</dataPath>
		<expression>ВЫБОР
    КОГДА ВидВремениОтсутствие ЕСТЬ NULL
        И (&amp;ДатаОкончания = &amp;ДатаОкончанияТрудозатрат
            ИЛИ (&amp;ДатаОкончания &lt;&gt; &amp;ДатаОкончанияТрудозатрат И ДатаКалендаря &lt;&gt; &amp;ДатаОкончанияТрудозатрат)) 
        ТОГДА ВЫБОР
        		КОГДА Уволен ТОГДА 0
                КОГДА ВидДняКалендарь = ЗНАЧЕНИЕ(Перечисление.ВидыДнейПроизводственногоКалендаря.Рабочий)
                    ТОГДА 8
                КОГДА ВидДняКалендарь = ЗНАЧЕНИЕ(Перечисление.ВидыДнейПроизводственногоКалендаря.Предпраздничный)
                    ТОГДА 7
                ИНАЧЕ 0
            КОНЕЦ
    КОГДА ПриоритетОтсутствия = 1
        И (&amp;ДатаОкончания = &amp;ДатаОкончанияТрудозатрат
            ИЛИ (&amp;ДатаОкончания &lt;&gt; &amp;ДатаОкончанияТрудозатрат И ДатаКалендаря &lt;&gt; &amp;ДатаОкончанияТрудозатрат))
        ТОГДА ОтработаноЧасов
    ИНАЧЕ 0
КОНЕЦ</expression>
